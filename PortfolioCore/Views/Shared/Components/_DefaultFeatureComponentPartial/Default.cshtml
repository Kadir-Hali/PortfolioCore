@model DefaultFeatureViewModel

<section id="hero" class="hero section light-background">

    <img src="/MyResume-1.0.0/assets/img/hero-bg.jpg" alt="">

    <div class="container" data-aos="zoom-out">
        <div class="row justify-content-center">
            <div class="col-lg-9">
                <h2>Kadir Halı</h2>
                <p>Çalışma Alanlarım : <span class="typed" data-typed-items="@string.Join(", ", Model.WorkFields)"></span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>
                <div class="social-links">
                    @foreach(var item in Model.SocialMedias)
                    {
                        <a href="@item.SocialMediaUrl"><i class="@item.SocialMediaIcon"></i></a>
                    }
                </div>
            </div>
        </div>
    </div>

</section>